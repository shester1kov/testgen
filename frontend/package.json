{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.tsx --fix",
    "format": "prettier --write src/",
    "design:enable": "bash scripts/enable-design-mode.sh",
    "design:disable": "bash scripts/disable-design-mode.sh",
    "design:build": "bash scripts/enable-design-mode.sh && npm run build"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@tailwindcss/vite": "^4.1.17",
    "axios": "^1.13.2",
    "pinia": "^3.0.4",
    "tailwindcss": "^4.1.17",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.47.0",
    "@typescript-eslint/parser": "^8.47.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitest/coverage-v8": "^4.0.10",
    "@vitest/ui": "^4.0.10",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.5.1",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.2.0",
    "prettier": "^3.6.2",
    "sass": "^1.94.1",
    "typescript": "~5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "vue-tsc": "^3.1.4",
    "wrangler": "^4.51.0"
  }
}
